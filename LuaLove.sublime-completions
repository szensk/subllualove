		{
			"trigger": "love.image.isCompressed",
			"annotation": "(filename)",
			"contents": "love.image.isCompressed(${1:filename})",
			"kind": "function",
			"details": "Determines whether a file can be loaded as CompressedImageData (<a href=\"https://love2d.org/wiki/love.image.isCompressed\">Manual</a>)"
